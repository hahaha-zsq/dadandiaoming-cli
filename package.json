{
  "name": "dadandiaoming-cli",
  "version": "0.1.20",
  "description": "一个脚手架工具 包含vue、react两种",
  "main": "index.js",
  "type": "module",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "version:patch": "scripts/update-version.sh patch",
    "version:minor": "scripts/update-version.sh minor",
    "version:major": "scripts/update-version.sh major",
    "version:check": "npm outdated",
    "lint": "echo 'Linting...' && echo 'No linter configured'",
    "format": "echo 'Formatting...' && echo 'No formatter configured'"
  },
  "keywords": [
    "脚手架",
    "React",
    "Vue3",
    "Vite",
    "TypeScript"
  ],
  "author": "dadandiaoming",
  "license": "ISC",
  "bin": {
    "dadandiaoming": "bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.1",
    "@types/prompts": "^2.4.9",
    "rollup": "^4.40.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "figlet": "^1.8.1",
    "fs-extra": "^11.3.0",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "simple-git": "^3.27.0",
    "tslib": "^2.8.1"
  }
}
